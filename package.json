{
	"name": "koa-react-native-web-desktop-universal-example",
	"version": "1.0.0",
	"scripts": {
		"build:server": "webpack --config source/res/server/build/webpack.prod.js --verbose --progress --hide-modules --colors",
		"debug:server": "webpack --config source/res/server/build/webpack.dev.js --verbose --progress --hide-modules --colors",
		"start:server": "cd dist/server && supervisor -w . --harmony index.js",
		"start:mock": "json-server -c config/mock.json -r mock/routes.json -w mock/db.json",
		"debug:mobile": "webpack-dev-server --config source/res/web/mobile/build/webpack.dev.js --watch --quiet --inline --hot --colors --progress",
		"build:mobile": "webpack --config source/res/web/mobile/build/webpack.prod.js --verbose --progress --hide-modules --colors",
		"start:native:test1": "cd dist/apps/test1 && npm run start",
		"start:ios:test1": "cd dist/apps/test1 && react-native run-ios",
		"start:android:test1": "cd dist/apps/test1 && react-native run-android",
		"debug:manage": "webpack-dev-server --config source/res/web/manage/build/webpack.dev.js --watch --quiet --inline --hot --colors --progress",
		"build:manage": "webpack --config source/res/web/manage/build/webpack.prod.js --verbose --progress --hide-modules --colors",
		"debug:website": "webpack-dev-server --config source/res/web/website/build/webpack.dev.js --watch --quiet --inline --hot --colors --progress",
		"build:website": "webpack --config source/res/web/website/build/webpack.prod.js --verbose --progress --hide-modules --colors"
	},
	"dependencies": {
		"co": "^4.6.0",
