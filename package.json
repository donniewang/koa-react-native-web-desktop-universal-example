		"koa-sslify": "^2.1.2",
		"koa-static-cache": "^5.1.1",
		"koa-views": "^6.0.2",
		"mime": "^1.3.6",
		"moment": "^2.18.1",
		"request": "^2.81.0",
		"sequelize": "^4.7.5",
		"sqlite3": "^3.1.9",
